<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <!-- Use the title from a page's frontmatter if it has one -->
    <title>Sponsaurus <%= " | " + current_page.data.title %></title>
    <link rel="stylesheet" href="https://rsms.me/inter/inter.css">
    <script async src="https://unpkg.com/vue@3.2.2/dist/vue.global.prod.js"></script>
    <%= favicon_tag 'favicon.ico' %>
    <%= stylesheet_link_tag "site" %>
    <%= generate_meta_tags(current_page) %>

    <%# only include tag in production %>
    <% if app.build? %>
      <%= partial 'partials/tracking_snippet' %>
    <% end %>

  </head>

  <body>
    <%= partial 'partials/_header.html.erb' %>
    <%= yield %>
    <%= partial 'partials/_footer.html.erb' %>

    
    <%= javascript_include_tag "site" %>
  </body>

</html>
